<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cole.binaryclockapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConversionActivity"
            android:label="@string/title_activity_conversion" >
        </activity>
        <activity
            android:name=".PracticeMode"
            android:label="@string/title_activity_practice_mode" >
        </activity>
        <activity
            android:name=".QuizActivity"
            android:label="@string/title_activity_quiz" >
        </activity>
        <activity
            android:name=".QuizDone"
            android:label="@string/title_activity_quiz_done" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="MainActivity" >
        </activity>
    </application>

</manifest>
